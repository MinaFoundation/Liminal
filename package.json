{
  "name": "liminal",
  "version": "0.0.0-local",
  "description": "",
  "repository": "github:MinaFoundation/Liminal",
  "license": "Apache-2.0",
  "type": "module",
  "bin": {
    "liminal": "./target/main.js"
  },
  "exports": {
    "liminal": {
      "default": "./target/mod.js",
      "types": "./target/mod.d.ts"
    }
  },
  "scripts": {
    "task": "node --enable-source-maps --no-warnings --loader ts-node/esm",
    "liminal": "npm run task main.ts",
    "test": "npm run liminal -- npm run task -- --test **.test.ts",
    "build": "tsc",
    "check": "tsc --no-emit",
    "codegen": "graphql-codegen --config ./graphql.config.ts",
    "postinstall": "npm run task _tasks/codegen.ts"
  },
  "dependencies": {
    "@graphql-tools/schema": "^10.0.3",
    "@graphql-tools/utils": "^10.1.2",
    "@std/assert": "npm:@jsr/std__assert@^0.221.0",
    "@std/async": "npm:@jsr/std__async@^0.221.0",
    "@std/cli": "npm:@jsr/std__cli@^0.221.0",
    "@std/fmt": "npm:@jsr/std__fmt@^0.221.0",
    "@std/fs": "npm:@jsr/std__fs@^0.221.0",
    "@std/http": "npm:@jsr/std__http@^0.221.0",
    "@std/path": "npm:@jsr/std__path@^0.221.0",
    "@std/yaml": "npm:@jsr/std__yaml@^0.221.0",
    "cids": "^1.1.9",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.12",
    "graphql": "^16.8.1",
    "graphql-playground-html": "^1.6.30",
